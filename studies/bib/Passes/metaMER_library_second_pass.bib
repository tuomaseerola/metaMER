  doi              = {10.3390/s21237854},
  hasabstract      = {Y},
  isbn             = {1424-8220},
  keywords         = {Emotion Recognition; Affective Computing; Emotion Regulation; Epilepsy Detection},
  la               = {en},
  modificationdate = {2024-05-16T06:35:35},
