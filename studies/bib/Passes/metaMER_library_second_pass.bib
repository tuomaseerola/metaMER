  doi           = {10.1109/tmm.2019.2922129},
  hasabstract   = {Y},
  isbn          = {1520-9210},
  keywords      = {Affective Computing; Emotion Recognition; Video Summarization; Action Recognition; Feature Extraction},
  la            = {en},
  priority      = {prio3},
  publisher     = {Institute of Electrical and Electronics Engineers},
