  doi              = {10.1109/access.2018.2883213},
  hasabstract      = {Y},
  isbn             = {2169-3536},
  keywords         = {Emotion Recognition; Affective Computing; Deep Learning for EEG; Deep Learning; Affective Design},
  la               = {en},
  modificationdate = {2024-05-16T06:35:35},
