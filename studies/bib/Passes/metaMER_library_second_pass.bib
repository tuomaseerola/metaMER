  date-modified = {2024-05-13 15:05:26 +0100},
  doi           = {10.3390/brainsci11111392},
  hasabstract   = {Y},
  isbn          = {2076-3425},
  keywords      = {Affective Computing; Emotion Recognition; Feature Extraction; Human-Computer Interaction; Aspect-based Sentiment Analysis},
  la            = {en},
