  doi              = {10.1109/taffc.2021.3060877},
  hasabstract      = {Y},
  isbn             = {1949-3045},
  keywords         = {Affective Computing; Emotion Recognition; General Game Playing; Player Modeling; Deep Learning},
  la               = {en},
  modificationdate = {2024-05-16T06:35:35},
