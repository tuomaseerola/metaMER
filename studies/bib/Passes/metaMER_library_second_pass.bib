  doi              = {10.1145/3233184},
  hasabstract      = {Y},
  isbn             = {1551-6857},
  keywords         = {Emotion Recognition; Affective Computing; Physiological Signals; Speech Emotion},
  la               = {en},
  modificationdate = {2024-05-16T06:35:35},
